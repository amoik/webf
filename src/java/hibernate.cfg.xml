<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
  <session-factory>
    <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
    <property name="hibernate.connection.driver_class">com.mysql.jdbc.Driver</property>
    <property name="hibernate.connection.url">jdbc:mysql://localhost:3306/web?zeroDateTimeBehavior=convertToNull</property>
    <property name="hibernate.connection.username">webf</property>
    <property name="hibernate.connection.password"></property>
    <mapping resource="at/webf/ws/PersonCourseMembership.hbm.xml"/>
    <mapping resource="at/webf/ws/Course.hbm.xml"/>
    <mapping resource="at/webf/ws/Person.hbm.xml"/>
    <mapping resource="at/webf/ws/CourseTeil.hbm.xml"/>
  </session-factory>
</hibernate-configuration>
